{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "svelte-img",
	"main": ".svelte-kit/cloudflare/_worker.js",
	"compatibility_date": "2025-05-05",
	"observability": {
		"enabled": true
	},
	"assets": {
		"binding": "ASSETS",
		"directory": ".svelte-kit/cloudflare"
	},
  "routes": [
    {
      "pattern": "svelte-img.moritz.website",
      "custom_domain": true
    }
  ]
}