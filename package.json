{
	"name": "@kage0x3b/svelte-img",
	"version": "3.0.1",
	"private": false,
	"description": "High-performance responsive/progressive images for SvelteKit",
	"repository": "github:Kage0x3B/svelte-img",
	"homepage": "https://svelte-img.moritz.website",
	"author": "Jason Lee <jason@zerodevx.com>",
	"contributors": [
		{
			"name": "Moritz Hein",
			"email": "moritz.hein@live.de",
			"url": "https://moritz.website"
		}
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm prepack",
		"preview": "wrangler pages dev .svelte-kit/cloudflare --ip 0.0.0.0 --port 5173",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "pnpm test:unit -- --run && pnpm test:e2e",
		"test:e2e": "playwright test",
		"cf-typegen": "wrangler types && mv worker-configuration.d.ts src/"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@fontsource-variable/inter": "^5.2.6",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify/tailwind": "^1.2.0",
		"@iconify/tailwind4": "^1.0.6",
		"@playwright/test": "^1.55.0",
		"@sveltejs/adapter-cloudflare": "^7.2.2",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.34.1",
		"@sveltejs/package": "^2.4.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"@vitest/browser": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"daisyui": "^5.0.50",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"imagetools-core": "^8.0.0",
		"playwright": "^1.55.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"publint": "^0.3.12",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.40.0",
		"vite": "^7.1.3",
		"vite-imagetools": "^8.0.0",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0",
		"wrangler": "^4.31.0"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./vite": {
			"types": "./dist/vite.d.ts",
			"default": "./dist/vite.js"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"license": "ISC",
	"keywords": [
		"svelte",
		"sveltekit",
		"responsive",
		"progressive",
		"image",
		"lazy-load"
	],
	"packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
