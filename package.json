{
  "name": "@zerodevx/svelte-img",
  "version": "1.2.7",
  "description": "High-performance responsive images for SvelteKit",
  "author": "Jason Lee <jason@zerodevx.com>",
  "scripts": {
    "dev": "NODE_ENV=development vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "test": "playwright test",
    "package": "svelte-kit sync && svelte-package && npx publint",
    "deploy": "npm run build && npx gh-pages -d build -t -f"
  },
  "dependencies": {
    "imagetools-core": "3.3.1",
    "vite-imagetools": "4.0.18"
  },
  "peerDependencies": {
    "svelte": "3.55.1"
  },
  "devDependencies": {
    "@playwright/test": "1.31.2",
    "@sveltejs/adapter-static": "2.0.1",
    "@sveltejs/kit": "1.10.0",
    "@sveltejs/package": "2.0.2",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.13",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-svelte3": "4.0.0",
    "is-port-reachable": "4.0.0",
    "postcss": "8.4.21",
    "postcss-load-config": "4.0.1",
    "prettier": "2.8.4",
    "prettier-plugin-svelte": "2.9.0",
    "svelte-check": "3.0.4",
    "svelte-preprocess": "5.0.1",
    "tailwindcss": "3.2.7",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "vite": "4.1.4"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./vite": "./dist/vite.js"
  },
  "svelte": "./dist/index.js",
  "files": [
    "dist"
  ],
  "license": "ISC",
  "repository": "github:zerodevx/svelte-img",
  "keywords": [
    "svelte",
    "sveltekit",
    "responsive",
    "progressive",
    "image",
    "lazy-load"
  ]
}
